    "using GR\n",
    "\n",
    "function hailLength(x::Int)\n",
    "    n = 0\n",
    "    while x != 1\n",
    "        if x % 2 == 0\n",
    "            = Int(x/2)\n",
    "        else\n",
    "            x = 3x +1\n",
    "        end\n",
    "        n += 1\n",
    "    end\n",
    "    return n\n",
    "end"
   ]
